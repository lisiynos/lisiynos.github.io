p = tf('p');
W=(2*p+8)/(p*(1+0.5*p)*(1+0.1*p));
[mag, phase,w] = bode(W);
figure;
semilogx(w, db(mag(:)));
grid;
%plot(ww,mag);
ww = w;  0.1 : 0.1 : 1000;
i = 1; 
for w=ww;
  A(i) = 20*log10(sqrt(64+20.64*w^2+1.2*w^4+0.01*w^6)/(w*(1+0.25*w^2)*(1+0.01*w^2))) + 2;  
  i = i + 1;
end;  
plot(w,A)
%plot(ww,A)
pole(W) % Полюса
zero(W) % Нули

